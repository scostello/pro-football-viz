{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": "off"
  },
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true,
    "node": true
  }
}